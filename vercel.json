{
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "./build/**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/$1"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "build/$1"
    }
  ]
}
